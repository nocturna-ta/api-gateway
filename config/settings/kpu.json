{
  "base_path": "",
  "endpoints": [
    {
      "path": "/v1/kpu-provinsi",
      "method": "GET",
      "host": "http://host.docker.internal:8900",
      "destination": "/v1/kpu-provinsi",
      "auth": {
        "log_request_on_error": true,
        "target_service": "kpu-provinsi-service"
      },
      "extra_config": {
        "qos/ratelimit/router": {
          "max_rate": 10,
          "client_max_rate": 3,
          "strategy": "ip"
        }
      },
      "backend_extra_config": {
        "qos/circuit-breaker": {
          "interval": 60,
          "timeout": 10,
          "max_errors": 1
        }
      }
    }
  ]
}