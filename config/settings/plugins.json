{
  "global": {
    "telemetry/logging": {
      "level": "INFO",
      "prefix": "[API-Gateway]",
      "syslog": false,
      "stdout": true
    },
    "telemetry/metrics": {
      "collection_time": "60s",
      "proxy_disabled": false,
      "router_disabled": false,
      "backend_disabled": false,
      "endpoint_disabled": false,
      "listen_address": ":8090"
    },
    "security/cors": {
      "allow_origins": [
        "*"
      ],
      "allow_methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH"
      ],
      "allow_headers": [
        "Authorization",
        "authorization",
        "Content-Type",
        "Accept",
        "x-api-key"
      ],
      "max_age": "6h"
    }
  }
}